{
  "name": "@ai-stats/devtools",
  "version": "0.2.1",
  "description": "Devtools for AI Stats SDK - enables telemetry capture and debugging",
  "type": "module",
  "private": false,
  "license": "MIT",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": false,
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsc -p tsconfig.build.json",
    "dev": "tsc -p tsconfig.build.json --watch"
  },
  "dependencies": {
    "@ai-stats/devtools-core": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^25.0.2",
    "rimraf": "^6.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.6.3"
  },
  "keywords": [
    "ai-stats",
    "devtools",
    "debugging",
    "telemetry",
    "monitoring",
    "observability"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AI-Stats/AI-Stats.git",
    "directory": "packages/devtools/devtools"
  },
  "publishConfig": {
    "access": "public"
  }
}
