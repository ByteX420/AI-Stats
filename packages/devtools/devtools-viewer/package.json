{
  "name": "@ai-stats/devtools-viewer",
  "description": "Web-based viewer for AI Stats DevTools telemetry",
  "version": "0.1.1",
  "type": "module",
  "private": false,
  "license": "MIT",
  "bin": {
    "ai-stats-devtools": "./dist/cli/index.js"
  },
  "main": "./dist/server/index.js",
  "types": "./dist/server/index.d.ts",
  "files": [
    "dist",
    "public",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "clean": "rimraf dist public",
    "build:server": "tsc -p tsconfig.server.json",
    "build:ui": "vite build",
    "build": "pnpm run build:ui && pnpm run build:server",
    "prepublishOnly": "pnpm run build",
    "dev:server": "pnpm exec tsx watch src/server/index.ts -- --start --port 4984 --dir ../../../.ai-stats-devtools",
    "dev:ui": "pnpm exec wait-on tcp:4984 && vite --port 4983",
    "dev": "node dev-start.mjs",
    "dev:old": "concurrently \"pnpm run dev:server\" \"pnpm run dev:ui\"",
    "start": "node dist/cli/index.js start",
    "test": "vitest"
  },
  "dependencies": {
    "@hono/node-server": "^1.16.0",
    "hono": "^4.11.9",
    "@tanstack/react-query": "^5.62.11",
    "lucide-react": "^0.544.0",
    "react": "^19.0.0",
    "react-dom": "^19.2.4",
    "recharts": "^2.15.0"
  },
  "devDependencies": {
    "@types/node": "^25.0.2",
    "@types/react": "^19.0.6",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.2",
    "postcss": "^8.4.49",
    "rimraf": "^6.0.0",
    "tailwindcss": "^3.4.18",
    "wait-on": "^7.2.0",
    "tsx": "^4.7.0",
    "typescript": "^5.6.3",
    "vite": "^6.0.5",
    "vitest": "^4.0.18"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AI-Stats/AI-Stats"
  },
  "publishConfig": {
    "access": "public"
  }
}
