{
  "name": "@ai-stats/ai-sdk-provider",
  "version": "1.0.1",
  "description": "AI Stats Gateway provider for Vercel AI SDK",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "postinstall": "node ../../sdk/scripts/postinstall-viewer-prompt.cjs @ai-stats/ai-sdk-provider",
    "build": "tsc -p tsconfig.build.json",
    "clean": "rimraf dist",
    "dev": "tsc -p tsconfig.build.json --watch",
    "test": "dotenv -e ../../../.env.local -- vitest run",
    "test:watch": "vitest",
    "prepublishOnly": "pnpm build"
  },
  "keywords": [
    "ai",
    "ai-sdk",
    "vercel",
    "ai-stats",
    "gateway",
    "llm",
    "openai",
    "anthropic",
    "language-model"
  ],
  "license": "MIT",
  "dependencies": {
    "@ai-sdk/provider": "^3.0.8",
    "@ai-sdk/provider-utils": "^4.0.15"
  },
  "devDependencies": {
    "@types/node": "^25.2.3",
    "@vitest/coverage-v8": "^4.0.18",
    "ai": "^6.0.91",
    "rimraf": "^6.0.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18",
    "zod": "^3.23.8"
  },
  "peerDependencies": {
    "ai": "^6.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AI-Stats/AI-Stats",
    "directory": "packages/integrations/ai-sdk-ai-stats"
  },
  "publishConfig": {
    "access": "public"
  }
}
